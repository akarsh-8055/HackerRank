#include <stdio.h>

void update(int *a,int *b) {
    // Complete this function    
}

int main() {
    int a, b,x,y;
    int *pa = &a, *pb = &b,*pc,*pd;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    pc=(*pa)+(*pb);
    x=abs(pc);
    pd=(*pa)-(*pb);
    y=abs(pd);
    printf("%d\n%d",x,y);

    return 0;
}
